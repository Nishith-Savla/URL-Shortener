<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         id="WebApp_ID" version="3.0">
  <display-name>Archetype Created Web Application</display-name>

  <welcome-file-list>
    <welcome-file>/src/index.jsp</welcome-file>
  </welcome-file-list>

  <servlet>
    <servlet-name>Shorten</servlet-name>
    <jsp-file>/src/shorten.jsp</jsp-file>
  </servlet>

  <servlet-mapping>
    <servlet-name>Shorten</servlet-name>
    <url-pattern>/shorten.jsp</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>edit</servlet-name>
    <jsp-file>/src/edit.jsp</jsp-file>
  </servlet>

  <servlet-mapping>
    <servlet-name>edit</servlet-name>
    <url-pattern>/edit.jsp</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>analytics</servlet-name>
    <jsp-file>/src/analytics.jsp</jsp-file>
  </servlet>

  <servlet-mapping>
    <servlet-name>analytics</servlet-name>
    <url-pattern>/analytics.jsp</url-pattern>
  </servlet-mapping>
</web-app>
